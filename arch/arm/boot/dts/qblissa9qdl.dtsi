	/*
	 * When using two IPUs:
 	 * HDMI can always go to IPU2 (id=1), DI1.
	 */
	disp_id = <1>;
	ipu_id = <0>;
#endif /* CONFIG_QBLISSA9_USE_ONE_IPU */
	status = "okay";
};
