#ifdef CONFIG_EFUSA9R2_HDMI
&hdmi_core {
#ifdef CONFIG_EFUSA9R2_USE_ONE_IPU
	/*
	 * When using one IPU (id=0):
	 * HDMI can go to DI1, unless LVDS1 is also active. In this case LVDS1
	 * must go to DI1 and HDMI to DI0.
	 */
	ipu_id = <0>;
#if (__HDMI_ON_0_OR_1__ && __LVDS1_ON_0_OR_1__)
	disp_id = <0>;
#else
	disp_id = <1>;
#endif
#else /* !CONFIG_EFUSA9R2_USE_ONE_IPU */
	/*
	 * When using two IPUs:
	 * HDMI can always go to DI1. If HDMI is on MXCFB0 and LCD on MXCFB1,
	 * or vice versa, or HDMI is on MXCFB2 and LCD is on MXCFB3, or vice
	 * versa, then HDMI has to go to IPU2 (id=1), otherwise to IPU1 (id=0).
	 */
	disp_id = <1>;
#if ((__HDMI_ON_0_OR_1__ && (__LCD_ON_0_OR_1__ || __BT656_ON_0_OR_1__)) \
    || (__HDMI_ON_2_OR_3__ && (__LCD_ON_2_OR_3__ || __BT656_ON_2_OR_3__)))
	ipu_id = <1>;
#else
	ipu_id = <0>;
#endif
#endif /* CONFIG_EFUSA9R2_USE_ONE_IPU */
	status = "okay";
};
#endif

#if (defined(CONFIG_EFUSA9R2_LVDS0) || defined(CONFIG_EFUSA9R2_LVDS1))
&ldb {
	ext_ref;
#if defined(CONFIG_EFUSA9R2_LVDS_SPLIT_MODE)
	split-mode;
#elif defined(CONFIG_EFUSA9R2_LVDS_DUAL_MODE)
	dual-mode;
#endif
	ldb-supply = <&reg_vlcd>;
	status = "okay";

#ifdef CONFIG_EFUSA9R2_LVDS0
	/* LVDS0 must always go to DI0 of an IPU! */
	lvds-channel@0 {
		reg = <0>;
#ifdef CONFIG_EFUSA9R2_USE_ONE_IPU
		crtc = "ipu1-di0";
#else
		crtc = "ipu2-di0";
#endif
		fsl,data-mapping = CONFIG_EFUSA9R2_LVDS0_MAPPING;
		fsl,data-width = <CONFIG_EFUSA9R2_LVDS0_DATA_WIDTH>;
#ifdef __LVDS0_PRIMARY__
		primary;
#endif
		status = "okay";

		display-timings {
			native-mode = <&timing0>;
			timing0: CONFIG_EFUSA9R2_LVDS0_TIMING;
		};
	};
#endif

#ifdef CONFIG_EFUSA9R2_LVDS1
	/* LVDS1 must always go to DI1 of an IPU! */
	lvds-channel@1 {
#if defined(CONFIG_EFUSA9R2_LVDS_DUAL_MODE) \
	|| defined(CONFIG_EFUSA9R2_LVDS_SPLIT_MODE)
		reg = <0>;
#else
		reg = <1>;
#endif
#if (defined(CONFIG_EFUSA9R2_USE_ONE_IPU) \
    || (__LVDS0_ON_0_OR_1__ && __LVDS1_ON_0_OR_1__) \
    || (__LVDS0_ON_2_OR_3__ && __LVDS1_ON_2_OR_3__))
		crtc = "ipu1-di1";
#else
		crtc = "ipu2-di1";
#endif
		fsl,data-mapping = CONFIG_EFUSA9R2_LVDS1_MAPPING;
		fsl,data-width = <CONFIG_EFUSA9R2_LVDS1_DATA_WIDTH>;
#ifdef __LVDS1_PRIMARY__
		primary;
#endif
		status = "okay";

		display-timings {
			native-mode = <&timing1>;
			timing1: CONFIG_EFUSA9R2_LVDS1_TIMING;
		};
	};
#endif
};
#endif /* CONFIG_EFUSA9R2_LVDS0 || CONFIG_EFUSA9R2_LVDS1 */

