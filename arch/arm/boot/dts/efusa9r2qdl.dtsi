/* RAM size for Continuous Memory Allocator; if not defined, use 320 MB */
#ifndef CONFIG_EFUSA9R2_CMA_SIZE
#define CONFIG_EFUSA9R2_CMA_SIZE 0x14000000
#endif
&cma {
	size = <CONFIG_EFUSA9R2_CMA_SIZE>;
};

&busfreq {
	/* Disable bus frequency scaling, because reducing bus frequency to
	   24 MHz does not work with all types of DDR3 RAM */
	disable-scaling;
};

&clks {
	assigned-clocks =
#ifdef CONFIG_EFUSA9R2_LVDS0_CLK
			  <&clks IMX6QDL_CLK_LDB_DI0_SEL>,
#endif
#ifdef CONFIG_EFUSA9R2_LVDS1_CLK
			  <&clks IMX6QDL_CLK_LDB_DI1_SEL>,
#endif
			  <&clks IMX6QDL_CLK_PLL4_AUDIO_DIV>,
			  <&clks IMX6QDL_CLK_CKO1_SEL>,
			  <&clks IMX6QDL_CLK_CKO1_PODF>,
			  <&clks IMX6QDL_CLK_CKO>;

	assigned-clock-parents =
#ifdef CONFIG_EFUSA9R2_LVDS0_CLK
				 <&clks CONFIG_EFUSA9R2_LVDS0_CLK>,
#endif
#ifdef CONFIG_EFUSA9R2_LVDS1_CLK
				 <&clks CONFIG_EFUSA9R2_LVDS1_CLK>,
#endif
				 <0>,
				 <&clks IMX6QDL_CLK_PLL4_AUDIO_DIV>,
				 <0>,
				 <&clks IMX6QDL_CLK_CKO1>;

	/*
	 * The Audio-PLL (PLL4) must have at least 650MHz. The PLL post divider
	 * can divide by 1, 2 and 4, which means the lowest possible frequency
	 * is 162.5 MHz. The largest PODF divider for CKO1 is 8, so we do not
	 * get below 20 MHz. Use 24.576 MHz as this is a multiple of the most
	 * commonly used bit rates.
	 *
	 * Remark: Solo and DualLite have an additional divider that also can
	 * divide by 1, 2 and 4. So here we can go down to 12.288 MHz.
	 */
	assigned-clock-rates =
#ifdef CONFIG_EFUSA9R2_LVDS0_CLK
				<0>,
#endif
#ifdef CONFIG_EFUSA9R2_LVDS1_CLK
				<0>,
#endif
#ifdef CONFIG_EFUSA9R2_AUDIO_12M288
				<98304000>,	/* Audio with 12.288 MHz */
				<98304000>,
				<12288000>;
#else
				<196608000>,	/* Audio with 24.576 MHz */
				<196608000>,
				<24576000>;
#endif
};

