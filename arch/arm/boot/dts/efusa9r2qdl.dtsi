#include <dt-bindings/pwm/pwm.h>

/*
 * When using one IPU, having LCD and LVDS0 at the same time is not possible,
 * because both devices would need to use display 0. But this can be simulated
 * by using LVDS1 instead of LVDS0 and activating Dual Mode.
 */
#ifdef CONFIG_EFUSA9R2_USE_ONE_IPU
#undef CONFIG_EFUSA9R2_MXCFB2
#undef CONFIG_EFUSA9R2_MXCFB3

#if (((CONFIG_EFUSA9R2_MXCFB0 == DISPLAY_LCD) \
      || (CONFIG_EFUSA9R2_MXCFB0 == DISPLAY_BT656)) \
     && (CONFIG_EFUSA9R2_MXCFB1 == DISPLAY_LVDS0))
#undef CONFIG_EFUSA9R2_MXCFB1
#define CONFIG_EFUSA9R2_MXCFB1		DISPLAY_LVDS1
#define __MOVE_LVDS0_TO_LVDS1_DUAL_MODE__
#endif

#if ((CONFIG_EFUSA9R2_MXCFB0 == DISPLAY_LVDS0) \
     && ((CONFIG_EFUSA9R2_MXCFB1 == DISPLAY_LCD) \
         || (CONFIG_EFUSA9R2_MXCFB1 == DISPLAY_BT656)))
#undef CONFIG_EFUSA9R2_MXCFB0
#define CONFIG_EFUSA9R2_MXCFB0		DISPLAY_LVDS1
#define __MOVE_LVDS0_TO_LVDS1_DUAL_MODE__
#endif

#ifdef __MOVE_LVDS0_TO_LVDS1_DUAL_MODE__
#undef CONFIG_EFUSA9R2_LVDS1_MAPPING
#undef CONFIG_EFUSA9R2_LVDS1_BPP
#undef CONFIG_EFUSA9R2_LVDS1_PIX_FMT
#undef CONFIG_EFUSA9R2_LVDS1_DATA_WIDTH
#undef CONFIG_EFUSA9R2_LVDS1_TIMING
#define CONFIG_EFUSA9R2_LVDS1_MAPPING	CONFIG_EFUSA9R2_LVDS0_MAPPING
#define CONFIG_EFUSA9R2_LVDS1_BPP		CONFIG_EFUSA9R2_LVDS0_BPP
#define CONFIG_EFUSA9R2_LVDS1_PIX_FMT	CONFIG_EFUSA9R2_LVDS0_PIX_FMT
#define CONFIG_EFUSA9R2_LVDS1_DATA_WIDTH	CONFIG_EFUSA9R2_LVDS0_DATA_WIDTH
#define CONFIG_EFUSA9R2_LVDS1_TIMING	CONFIG_EFUSA9R2_LVDS0_TIMING
#ifndef CONFIG_EFUSA9R2_LVDS_DUAL_MODE
#define CONFIG_EFUSA9R2_LVDS_DUAL_MODE
#endif
#endif
#endif /* CONFIG_EFUSA9R2_USE_ONE_IPU */

/* Do we have LCD? */
#if ((CONFIG_EFUSA9R2_MXCFB0 == DISPLAY_LCD) \
    || (CONFIG_EFUSA9R2_MXCFB1 == DISPLAY_LCD) \
    || (CONFIG_EFUSA9R2_MXCFB2 == DISPLAY_LCD) \
    || (CONFIG_EFUSA9R2_MXCFB3 == DISPLAY_LCD))
#define CONFIG_EFUSA9R2_LCD
#if ((CONFIG_EFUSA9R2_MXCFB0 == DISPLAY_LCD) \
    || (CONFIG_EFUSA9R2_MXCFB1 == DISPLAY_LCD))
#define __LCD_ON_0_OR_1__	1
#define __LCD_ON_2_OR_3__	0
#else
#define __LCD_ON_0_OR_1__	0
#define __LCD_ON_2_OR_3__	1
#endif
#endif

/* Do we have BT656? */
#if ((CONFIG_EFUSA9R2_MXCFB0 == DISPLAY_BT656) \
    || (CONFIG_EFUSA9R2_MXCFB1 == DISPLAY_BT656) \
    || (CONFIG_EFUSA9R2_MXCFB2 == DISPLAY_BT656) \
    || (CONFIG_EFUSA9R2_MXCFB3 == DISPLAY_BT656))
#define CONFIG_EFUSA9R2_BT656
#if ((CONFIG_EFUSA9R2_MXCFB0 == DISPLAY_BT656) \
    || (CONFIG_EFUSA9R2_MXCFB1 == DISPLAY_BT656))
#define __BT656_ON_0_OR_1__	1
