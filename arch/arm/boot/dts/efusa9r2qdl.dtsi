/* Do we have HDMI? */
#if ((CONFIG_EFUSA9R2_MXCFB0 == DISPLAY_HDMI) \
    || (CONFIG_EFUSA9R2_MXCFB1 == DISPLAY_HDMI) \
    || (CONFIG_EFUSA9R2_MXCFB2 == DISPLAY_HDMI) \
    || (CONFIG_EFUSA9R2_MXCFB3 == DISPLAY_HDMI))
#define CONFIG_EFUSA9R2_HDMI
#define CONFIG_EFUSA9R2_HDMI_AUDIO
#define CONFIG_EFUSA9R2_HDMI_CEC
#if ((CONFIG_EFUSA9R2_MXCFB0 == DISPLAY_HDMI) \
