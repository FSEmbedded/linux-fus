#ifdef CONFIG_EFUSA9R2_LCD
	lcd: lcd@0 {
		compatible = "fsl,lcd";
		lcd-supply = <&reg_vlcd>;
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_ipu1_ctrl &pinctrl_ipu1_dat>;
		status = "disabled";
	};
#endif

#ifdef CONFIG_EFUSA9R2_BT656
	bt656: bt656@0 {
		compatible = "fsl,bt656";
		default_ifmt = "BT656";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_ipu1_bt656>;
		status = "disabled";
	};
#endif

	/* RGB-LCD backlight PWM on RGB adapter */
	backlight_lcd {
		compatible = "pwm-backlight";
		pwms = <&pca963x 1 640000 0>;
		power-supply = <&reg_lcd_bl>;
		brightness-levels = <0 1 5 10 18 28 41 56
				     73 92 113 137 163 192 222 255>;
		default-brightness-level = <14>;
		fb-names = "lcd";
	};

	/* LVDS backlight PWM on backlight connector */
	backlight_ldb {
		compatible = "pwm-backlight";
		power-supply = <&reg_ldb_bl>;
#ifndef DISPLAY_LVDS_CHIMEI
		pwms = <&pwm3 0 250000 PWM_POLARITY_INVERTED>;
		brightness-levels = <135 143 151 159 167 175 183 191
				     199 207 215 223 231 239 247 255>;
#else
		pwms = <&pwm3 0 5000000 0>;
		brightness-levels = <0 1 5 10 18 28 41 56
				     73 92 113 137 163 192 222 255>;
#endif
		default-brightness-level = <14>;
		fb-names = "ldb0", "ldb1";
	};

#ifdef CONFIG_EFUSA9R2_CAMERA
	/* First capture device, for parallel camera or first MIPI channel */
	v4l2_cap_0 {
		compatible = "fsl,imx6q-v4l2-capture";
		ipu_id = <0>;
		csi_id = <0>;
		mclk_source = <0>;
		status = "okay";
	};

#ifdef CONFIG_EFUSA9R2_SERIAL_CAMERA
	/* Second capture device, for second MIPI channel */
	v4l2_cap_1 {
		compatible = "fsl,imx6q-v4l2-capture";
		ipu_id = <0>;
		csi_id = <1>;
		mclk_source = <0>;
		status = "okay";
	};

#ifndef CONFIG_EFUSA9R2_USE_ONE_IPU
	/* Third capture device, for third MIPI channel */
	v4l2_cap_2 {
		compatible = "fsl,imx6q-v4l2-capture";
		ipu_id = <1>;
		csi_id = <0>;
		mclk_source = <0>;
		status = "okay";
	};

