	picocoremx6sx {
		pinctrl_hog: hoggrp {
			fsl,pins = <
				/* GPIO */
				MX6SX_PAD_ENET2_TX_CLK__GPIO2_IO_9	0x030b0
				MX6SX_PAD_KEY_COL0__GPIO2_IO_10		0x030b0
				MX6SX_PAD_KEY_COL1__GPIO2_IO_11		0x030b0
				MX6SX_PAD_KEY_COL3__GPIO2_IO_13		0x030b0
