	dual-mode;
#endif
	ldb-supply = <&reg_vldb>;
	status = "okay";

#ifdef CONFIG_QBLISSA9R2_LVDS0
	/* LVDS0 must always go to DI0 of an IPU! */
	lvds-channel@0 {
