		fsl,data-mapping = CONFIG_QBLISSA9R2_LVDS0_MAPPING;
		fsl,data-width = <CONFIG_QBLISSA9R2_LVDS0_DATA_WIDTH>;
#ifdef __LVDS0_PRIMARY__
		primary;
#endif
		status = "okay";

		display-timings {
			native-mode = <&timing0>;
