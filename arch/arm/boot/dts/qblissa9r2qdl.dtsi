		fsl,data-width = <CONFIG_QBLISSA9R2_LVDS1_DATA_WIDTH>;
#ifdef __LVDS1_PRIMARY__
		primary;
#endif
		status = "okay";

		display-timings {
			native-mode = <&timing1>;
			timing1: CONFIG_QBLISSA9R2_LVDS1_TIMING;
		};
	};
#endif
};

